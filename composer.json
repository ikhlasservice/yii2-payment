{
    "name": "ikhlasservice/yii2-payment",
    "description": "Yii2 for payment management system.",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "GPL-3.0",
    "minimum-stability": "stable",
    "support": {
        "source": "https://github.com/ikhlasservice/yii2-payment.git",
        "issues": "https://github.com/ikhlasservice/yii2-payment/issues"
    },
    "authors": [
        {
            "name": "Ikhlas Service",
            "email": "ahamad.jedu@gmail.com",
            "role" : "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "andatech/yii2-core": "*"
    },
    "autoload": {
        "psr-4": {
            "ikhlas\\payment\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
